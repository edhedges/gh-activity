{
    "name": "gh-activity",
    "version": "1.0.0",
    "description": "GitHub activity summary",
    "main": "index.js",
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "build:lambda": "rm -rf build && ./node_modules/.bin/tsc && cp -R node_modules build/node_modules && cd build; zip -r ../build.zip *",
        "dev": "./node_modules/nodemon/bin/nodemon.js",
        "start": "node -e 'require(\"./build/index.js\").handler()'",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Eddie Hedges",
    "license": "UNLICENSED",
    "devDependencies": {
        "@types/node": "^11.13.0",
        "nodemon": "^1.18.10",
        "ts-node": "^8.0.3",
        "typescript": "^3.4.2"
    },
    "dependencies": {
        "@octokit/rest": "^16.23.2",
        "axios": "^0.18.0",
        "dotenv": "^7.0.0",
        "moment": "^2.24.0"
    }
}
